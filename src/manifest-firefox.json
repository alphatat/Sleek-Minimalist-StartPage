{
  "manifest_version": 2,
  "name": "SleekStartpage",
  "version": "1.0",
  "description": "A minimalist startpage or new Tab page to get the job done.",

  "chrome_url_overrides": {
    "newtab": "site  },

  "permissions": [
    "storage"
  ],

  "web_accessible_resources": [
    "site  ],

  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },

  "applications": {
    "gecko": {
      "id": "sleekstartpage@example.com",
      "strict_min_version": "91.0"
    }
  }
}

