{
  "manifest_version": 2,
  "name": "SleekStartpage",
  "version": "0.0.1",
  "author": "alphaTat",
  "description": "A minimalist startpage or new Tab page to get the job done.",
  "chrome_url_overrides": {
    "newtab": "site/index.html"
  },
  "permissions": [
    "storage"
  ],
  "web_accessible_resources": [
    "site/*"
  ],
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "applications": {
    "gecko": {
      "id": "sleekstartpage@alphatat.com",
      "strict_min_version": "91.0"
    }
  }
}
